func add(int x, int y, int z) -> int {
	test();
	return x + y + z;
}

func test() {
}

main {
	var x:int = 1;
	x += 1;
	
	x = 1 + 2 * 3 + 6 / 2;
	
	//check if x is equals to 2 if so add 3.156 and 5 to it
	if(x == 2 && true) {
		var y:int = 3.156;
		x = add(x, y, 5);
	} else if (x > 10) {
		x -= 10;
	} else {
		x -= 3;
	}
	
	while(x < 100) {
		x += 1;
		break;
	}
}